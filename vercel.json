{
  "rewrites": [
    { "source": "/telegram.org/(css|js|img)/(.+)", "destination": "https://telegram.org/$1/$2" },
    { "source": "/cdn(\\d).cdn-telegram.org/(.+)", "destination": "https://cdn$1.cdn-telegram.org/$2" },
    { "source": "/img/tgme/pattern.svg", "destination": "https://telegram.org/img/tgme/pattern.svg" },
    { "source": "/v/", "destination": "https://t.me/v/" },
    { "source": "/(.*)", "destination": "/api" }
  ]
}
